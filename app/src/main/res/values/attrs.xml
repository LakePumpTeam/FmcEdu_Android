<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="TopBarControl">
        <attr name="backVisible" format="boolean" />
        <attr name="topTitle" format="reference|string" />
        <attr name="operatorText" format="reference|string" />
        <attr name="operatorImageSrc" format="reference" />
    </declare-styleable>
    <declare-styleable name="ValidateButtonControl">
        <attr name="repeatSendTime" format="integer" />
    </declare-styleable>
    <declare-styleable name="MenuItemControl">
        <attr name="menuName" format="string|reference" />
        <attr name="drawableId" format="reference"></attr>
    </declare-styleable>
    <declare-styleable name="ExpandableTextViewControl">
        <attr name="isExpand" format="boolean" />
        <attr name="expand" format="string|reference" />
        <attr name="collapse" format="string|reference" />
        <attr name="maxCollapseLines" format="integer|reference" />
    </declare-styleable>
</resources>